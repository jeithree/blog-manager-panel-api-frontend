		"cors": "^2.8.5",
		"express": "^4.21.1",
		"express-rate-limit": "^8.2.1",
		"express-session": "^1.18.2",
		"helmet": "^8.0.0",
		"luxon": "^3.7.2",
		"multer": "^2.0.2",
		"node-schedule": "^2.1.1",
